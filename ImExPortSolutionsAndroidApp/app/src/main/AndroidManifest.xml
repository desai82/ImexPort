<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.imexportsolutions.imexportsolutions">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_star_new"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CompletedPoDetails"></activity>
        <activity android:name=".forgotpswd" />
        <activity
            android:name=".DocUpload"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".LogInPage"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SplashScreen"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SettingsActivity"
            android:label="@string/title_activity_settings"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MyProfile"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".QrcodeScanner"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".WaitingPODetails"
            android:label="Waiting P.O. Details" />
        <activity
            android:name=".ReceivedPODetails"
            android:label="Received P.O. Details" />
    </application>

</manifest>