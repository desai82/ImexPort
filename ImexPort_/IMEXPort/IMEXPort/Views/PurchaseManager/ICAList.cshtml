
@{
	
		Layout = "~/Views/Shared/_LayoutTemplateManager.cshtml";
	
}

<div class="content-wrapper">
	<div class="row">
		<di class="col-lg-12 grid-margin stretch-card">
			<div class="card">
				<div class="card-body">

					
					<h1 class="card-title">ICA List</h1>

					<div class="table-responsive">
						<table class="table">
							<thead>
								<tr>
									<th>ID</th>
									<th>Name</th>
									<th>Email</th>
									<th>Mobile</th>
									<th>Active</th>
									<th>Country</th>
									<th>Port</th>
								</tr>
							</thead>
							<tbody>

								@for (int i = 0; i < Model.Rows.Count; i++)
								{
								<tr>

									<td>@Model.Rows[i]["CustomsAgentID"]</td>
									<td>@Model.Rows[i]["Name"]</td>
									<td>@Model.Rows[i]["Email"]</td>
									<td>@Model.Rows[i]["Mobile"]</td>
									<td>@Model.Rows[i]["IsActive"]</td>
									<td>@Model.Rows[i]["Country"]</td>
									<td>@Model.Rows[i]["Port"]</td>

									<td><a href="ICAEdit?CustomsAgentID=@Model.Rows[i]["CustomsAgentID"]">Edit</a></td>





								</tr>

								}



							</tbody>
						</table>
					</div>
				</div>
			</div>
		</di>
	</div>
</div>



