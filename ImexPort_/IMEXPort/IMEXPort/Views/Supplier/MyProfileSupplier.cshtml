
@{
    Layout = "~/Views/Shared/_LayoutTemplateManager.cshtml";
}


<div class="content-wrapper">
    <h1 class="justify-content-center">
        Welcome @ViewBag.CurrentSupplier.Name
        <img class="img-lg rounded-circle" src="~/ProfilePhotos/SupplierPics/@ViewBag.CurrentSupplier.Photo" alt="Profile image">
    </h1>

    <div class="row">
        <div class="col-9 grid-margin">
            <div class="card">
                <div class="card-body">
                    <br />
                    <h5 class="card-title">
                        SupplierID : @ViewBag.CurrentSupplier.SupplierID
                    </h5><br />
                    <h5 class="card-title">
                        Name : @ViewBag.CurrentSupplier.Name
                    </h5><br />
                    <h5 class="card-title">
                        Email : @ViewBag.CurrentSupplier.Email
                    </h5><br />
                    <h5 class="card-title">
                        Mobile : @ViewBag.CurrentSupplier.Mobile
                    </h5><br />
                    <h5 class="card-title">
                        Username : @ViewBag.CurrentSupplier.Username
                    </h5>	<br />
                    <h5 class="card-title">
                        Password : @ViewBag.CurrentSupplier.Password
                    </h5>	<br />
                    <h5 class="card-title">
                        Username : @ViewBag.CurrentSupplier.Country
                    </h5>	<br />
                </div>
            </div>
        </div>
    </div>
</div>

